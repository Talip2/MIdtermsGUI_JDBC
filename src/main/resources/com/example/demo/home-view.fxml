<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
   <children>
      <VBox fx:id="pnLogout" prefHeight="400.0" prefWidth="600.0">
         <children>
            <BorderPane prefHeight="402.0" prefWidth="600.0">
               <top>
                  <ColorPicker fx:id="btnColorPicker" onAction="#onChoose" BorderPane.alignment="TOP_LEFT">
                     <BorderPane.margin>
                        <Insets left="50.0" top="50.0" />
                     </BorderPane.margin>
                  </ColorPicker>
               </top>
               <bottom>
                  <Button fx:id="logout" mnemonicParsing="false" onAction="#LogOut" text="Logout" BorderPane.alignment="TOP_RIGHT">
                     <BorderPane.margin>
                        <Insets bottom="25.0" right="25.0" />
                     </BorderPane.margin></Button>
               </bottom>
               <left>
                  <VBox prefHeight="200.0" prefWidth="150.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="btnDarkmode" mnemonicParsing="false" onAction="#darkMode" text="Dark mode">
                           <VBox.margin>
                              <Insets left="50.0" top="50.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="btnLebron" mnemonicParsing="false" onAction="#goBackToLebron" text="Lebron James">
                           <VBox.margin>
                              <Insets left="50.0" top="30.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </left>
            </BorderPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
